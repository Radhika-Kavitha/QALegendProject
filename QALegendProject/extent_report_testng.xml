<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Functional Suite">
	<listeners>
		<listener class-name="org.qalegend.listeners.Extent_Listeners" />
		<listener class-name="org.qalegend.listeners.Annotation_Transformer"/>
	</listeners>
	<test name="Test">
		<parameter name="browser" value="Chrome" />
		<parameter name="baseurl"
			value="https://qalegend.com/billing/public/login" />
		<classes>
			<class name="test.LoginPageTest" />
			<class name="test.ResetPageTest" />
			<class name="test.HomePageTest" />
			<class name="test.UserManagementPageTest" />
			<class name="test.UsersPageTest" />
			<class name="test.AddUserPageTest" />
		</classes>
	</test>


</suite>